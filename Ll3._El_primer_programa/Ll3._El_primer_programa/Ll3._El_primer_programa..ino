//******  Includes  ******************************************************


//******  Variables  *****************************************************
int ledPlacaPin = 10;


//******  Setup  ********************************************************* 
void setup() {
pinMode(ledPlacaPin, OUTPUT);
}


//******  Loop  ********************************************************** 
void loop() {
digitalWrite(ledPlacaPin, HIGH);   
delay(1000);             
digitalWrite(ledPlacaPin, LOW);    
delay(1000);              
}
